#%RAML 1.0 DataType
description: Base product for searching
type: object
properties:
  productId:
    type: integer
    required: false
    example: 801
  name:
    type: string
    required: false
    example: "Jeep Wrangler"
  summary:
    type: string
    required: false
    example: "A Jeep Wrangler."
  manufacturer:
    type: string
    example: "Jeep"
    required: false
  averageRating:
    type: number
    required: false
    example: 3.75
